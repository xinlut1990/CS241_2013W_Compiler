main
  var a, b;
  array[6] x;
  
{
  let a <- call InputNum(); 
  let b <- call InputNum();
  let x[5] <- call InputNum();
  let a <- a + 1;
  let b <- b + x[5]; 
  if a > 0 then
    let b <- a;
    let x[5] <- 3
  fi;
  let b <- b + a + x[5];   
  let b <- b + x[5];
  call OutputNum(b)
}.
